<h1><%= @user.email %></h1>

<p>Account created on <%= @user.created_on %>.</p>

<h2><%= @user.email %>'s Recipes</h2>

<ul>
  <% @user.recipes.each do |recipe| %>
    <li><%= link_to recipe.name, recipe_path(recipe) %></li>
  <% end %>
</ul>




